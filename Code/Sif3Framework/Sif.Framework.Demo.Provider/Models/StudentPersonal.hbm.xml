<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Sif.Framework.Demo.Provider" namespace="Sif.Framework.Demo.Provider.Models">
  <class name="StudentPersonal" table="STUDENT_PERSONAL" lazy="false">
    <id name="Id" type="Guid" column="STUDENT_PERSONAL_ID">
      <generator class="guid"/>
    </id>
    <property name="LocalId" column="LOCAL_ID" length="32"/>
    <many-to-one name="PersonInfo" column="PERSON_INFO_ID" class="Sif.Framework.Demo.Provider.Models.PersonInfo" cascade="all" fetch="join" unique="true"/>
  </class>
</hibernate-mapping>
