<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Sif.Framework" namespace="Sif.Framework.Model.Infrastructure">
  <class name="ProductIdentity" table="PRODUCT_IDENTITY">
    <id name="Id" column="PRODUCT_IDENTITY_ID">
      <generator class="native"/>
    </id>
    <property name="VendorName" column="VENDOR_NAME" length="256"/>
    <property name="ProductName" column="PRODUCT_NAME" length="256"/>
    <property name="ProductVersion" column="PRODUCT_VERSION" length="80"/>
    <property name="IconURI" column="ICON_URI" length="256"/>
  </class>
</hibernate-mapping>
