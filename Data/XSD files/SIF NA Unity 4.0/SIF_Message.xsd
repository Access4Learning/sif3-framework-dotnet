        </xs:key>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="RefId" use="required" type="RefIdType">
      <xs:annotation>
        <xs:appinfo>
          <sifChar>M</sifChar>
          <privacyRating />
          <cedsId>000364</cedsId>
          <cedsURL>https://ceds.ed.gov/element/000364</cedsURL>
        </xs:appinfo>
        <xs:documentation>The GUID that uniquely identifies this object.</xs:documentation>
